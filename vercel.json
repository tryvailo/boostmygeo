{
  "functions": {
    "api/main.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/main.py"
    }
  ],
  "build": {
    "env": {
      "PIP_NO_DEPS": "1",
      "PIP_IGNORE_INSTALLED": "1"
    },
    "installCommand": "pip install -r requirements.txt"
  }
}
