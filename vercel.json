{
  "build": {
    "os": "ubuntu-20.04",
    "commands": [
      "pip install -r requirements.txt"
    ]
  },
  "functions": {
    "api/main.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/main.py"
    }
  ]
}